// Write your function here:

const toEmoticon = string  => {
  switch (string) {
    case string = 'shrug':
      return '|_{"}_|'
    case string = 'smiley face':
      return ':)'
    case string = 'frowny face':
      return ':('
    case string = 'winky face':
      return ';)'
    case string = 'heart':
      return '<3'
    default:
      return '|_(* ~ *)_|'
  }
  
}

// Uncomment the line below when you're ready to try out your function
 console.log(toEmoticon("whatever")) 
// Should print  '|_(* ~ *)_|'

