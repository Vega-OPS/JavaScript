let raceNumber = Math.floor(Math.random() * 1000);

let runnerRegistered = false;

let runnersAge = 18;

if (runnersAge > 18 && runnerRegistered == true) {
  console.log(raceNumber += 1000);
}

if (runnersAge > 18 && runnerRegistered == true) {
  console.log(raceNumber + ' will race at 9:30 am')
}
else if (runnersAge > 18 && runnerRegistered == false) {
  console.log(raceNumber + ' will race at 11:00 am')
} 
else if (runnersAge < 18) {
  console.log(raceNumber + ' will race at 12:30 pm')
}
else if (runnersAge === 18) {
  console.log('Go to registration desk.')
}
